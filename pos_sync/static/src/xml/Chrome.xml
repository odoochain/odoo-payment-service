<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-inherit="point_of_sale.Chrome" t-inherit-mode="extension" owl="1">
        <xpath expr="//img[@class='pos-logo']" position="attributes">
            <attribute name="class">pos-logo d-none</attribute>
        </xpath>
        <xpath expr="//div[@class='pos-topheader']//CashMoveButton" position="before">
            <SyncButton/>
        </xpath>
        <xpath expr="//div[@class='pos-rightheader']//CashMoveButton" position="before">
            <SyncButton t-if="env.isMobile"/>
        </xpath>
    </t>
</templates>